<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.my.hermes.dao.MapMapper">

	
	
	<insert id="mapwrite" parameterType="mapinfo">
	insert into
		hermes_map
	values (
		#{map_title},
		sysdate,
		#{user_id},
		#{map_origin},
		#{map_destination},
		#{waypoint_inputArr0},		
		#{waypoint_inputArr1},
		#{waypoint_inputArr2},
		#{waypoint_inputArr3},
		#{waypoint_inputArr4}
	)
	</insert>
	
		

	
</mapper>